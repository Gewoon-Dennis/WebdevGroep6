@page
@model DatabaseImplementatie.Pages.GetArtists

@{ 
    ViewData["Title"] = "Artiest Ophalen";
}

@{
    foreach (var schrijver in Model.SchrijverList)
    {
        <li>Naam: @schrijver.schrijver_naam</li>
        <li>Geslacht: @schrijver.geslacht</li>
        <li>Wikipedia: @schrijver.wikipedia_schrijver</li>
        <li>Geboortedatum: @schrijver.geboortedatum</li>
    }

    foreach (var tekenaar in Model.TekenaarList)
    {
        <li>Naam: @tekenaar.tekenaar_naam</li>
        <li>Geslacht: @tekenaar.geslacht</li>
        <li>Wikipedia: @tekenaar.wikipedia_tekenaar</li>
        <li>Geboortedatum: @tekenaar.geboortedatum</li>
    }
}